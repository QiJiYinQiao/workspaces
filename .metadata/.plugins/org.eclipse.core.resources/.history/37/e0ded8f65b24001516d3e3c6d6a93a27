<script type="text/javascript">
var $row;
var $creditAuditReport;
var attempData;
$(function(){
	// 查看申请状态
	$row = parent.$.modalDialog.openner.datagrid('getSelected');
	$("#lookLoanOrderdg").datagrid({
		url : "loanOrderHis/loanOrderHisAction!findAllLoanOrderHis.action",
		width : 'auto',
		height : $(this).height()*0.34,
		pagination:true,
		rownumbers:true,
		border:true,
		singleSelect:true,
		nowrap:true,
		queryParams:{"loanOrderId":$row.loanOrderId},
		multiSort:false,
		columns : [ [ 
		              {field : 'agentTime',title : '受理时间',width : parseInt($(this).width()*0.1),sortable:true},
		              {field : 'roleName',title : '受理角色',width : parseInt($(this).width()*0.1)},
		              {field : 'assigneeName',title : '受理人',width : parseInt($(this).width()*0.1),align : 'left'},
		              {field : 'comment',title : '审批意见',width :parseInt($(this).width()*0.1),align : 'left'}
		              ] ]
	});
	// 渲染姓名
	$("#acceptTaskForm input[name='name']").val($row.name);
	// 渲染身份证号
	$("#acceptTaskForm input[name='idNo']").val($row.idNo);
	
	//根据订单ID查询所有联系人信息 
	$("#contacts-div").empty();
	$.ajax({
		url : "contacts/contactsAction!findContactsByOid.action",
		data : {
			loanOrderId : $row.loanOrderId
		},
		type : "post",
		success : function(data) {
			$.each(data,function(i,item){
				var formid = "contacts-form"+i;
				$("#contacts-form-model").clone()
				.attr("id",formid)
				.appendTo("#contacts-div");
				$("#"+formid+" span:first").html(i+1);
				$("#"+formid+" input[name='name']").val(item.chName).attr({"class":"easyui-textbox easyui-validatebox","data-options":"required:true"});
				$("#"+formid+" input[name='contactMethod']").val(item.tel).attr({"class":"easyui-textbox easyui-validatebox","data-options":"required:true,validType:'mobile'"});
				$("#"+formid+" input[name='auditItem']").val("audit_"+item.relationship);
				$("#"+formid+" input[name='relationship']").val(item.relationship).attr("class","easyui-textbox");
				$("#"+formid+" textarea:eq(0)").attr("class","easyui-textbox");
				$("#"+formid+" textarea:eq(1)").attr("class","easyui-textbox");
				$("#"+formid+" div:eq(0)").children("a").attr({"class":"easyui-linkbutton","iconCls":"icon-save"});
				$("#"+formid+" div:eq(1)").attr("id","creditIversTelAttrList"+i);
				$("#"+formid+" div:eq(2)").attr("id","contacts_upload_add"+i);
				$("#"+formid+" div:eq(3)").attr("id","customerInfo_upload_father_idDiv"+i);
				$("#"+formid+" div:eq(4)").attr("id","contacts_upload_div"+i);
				$("#contacts_upload_div"+i+" input:first").attr("class","easyui-combobox");
				$("#contacts_upload_div"+i+" a:first").attr({"class":"easyui-linkbutton","iconCls":"icon-add","plain":"true"});
				$("#contacts_upload_div"+i+" a:last").attr({"class":"easyui-linkbutton","iconCls":"icon-remove","plain":"true"});
				$("#contacts_upload_add"+i).next().children("a").attr("class","easyui-linkbutton");
				
				$.parser.parse('#'+formid); 
				
				$("#contacts_upload_div"+i+" input:first").combobox({
					valueField : 'code',
					textField : 'text',
					data : attempData,
					editable:false ,
			    });
				// $("#contacts_upload_div"+i+" span:eq(3)").remove();
			});
		},
		error : function(){
			
		}
	});
	
	loadAttachmentList('attachmentList','noauditId');
	
	$("#upload_form_div input:first").combobox({
		valueField : 'code',
		textField : 'text',
		url:'common/commonAction!findTextArr.action?codeMyid=attachment_type',
		onLoadSuccess : function(){
			attempData = $("#upload_form_div input:first").combobox("getData");
		},
		editable:false ,
    });
	
	
});
	
		//加载附件列表 
		function loadAttachmentList(listId,auditId){
			$("#"+listId).empty();
			$.ajax({
				url : "attachment/attachmentAction!findAttachmentByULA.action",
				data : {"orderType":"borrowerOrder","auditId":auditId},
				type : "post",
				async : false,
				success : function(data){
					var attId;
					var attName;
					var attType;
					var linkStr = "";
					var j = 0;
					$.each(data,function(i,item){
						attId = data[i][0];
						attName = data[i][1];
						attType = data[i][2];
						linkStr = "<div style='width:50%;height:30px;float: left;'><a href='javascript:void(0);'><span class='linkSpan'>"+attName+"</span></a><a href='javascript:void(0);'><span class='linkSpan'>"+attType+"</span></a><a href='javascript:void(0);' class='attachBackLinkButton' onclick=\"deleteAttachment(this,'"+attId+"');\"><span class='linkSpanS'>删除</span></a></div>";
						$("#"+listId).append(linkStr);
					});
				},
				error : function(){
					
				}
			});
		}	
		

		var j = 0;
		//增加一条同类型附件
		function addACredential(obj){
			j++;
			var pDiv = obj.parentElement;
			var pDivId = pDiv.id.split("_")[0];
			$(pDiv.parentElement).append("<div id='"+pDivId+"_"+j+"'>"+$(pDiv).html()+"</div>");
			$("#"+pDivId+"_"+j+" input:first").combobox({
				valueField : 'code',
				textField : 'text',
				data : attempData,
				editable : false,
		    });
			var fileId = $("#"+pDivId+"_"+j+" input:last").attr("id");
			$("#"+pDivId+"_"+j+" input:last").attr("id",fileId+""+j);
			$("#"+pDivId+"_"+j+" span:eq(3)").remove();
		}
		
		//删除一条同类型附件
		function removeACredential(obj){
			var pDiv = obj.parentElement;
			var pDivid = pDiv.id;
			var ppDiv = $(obj).parent().parent();
			if(ppDiv.children().length==1){
				return false;
			}
			$(pDiv).remove();
		}
		
		//删除一条附件
		function deleteAttachment(obj,attId){
			$.messager.confirm('提示','确认删除此项?',function(r){   
			    if (r){   
			    	$.ajax({
				    	   url:'attachment/attachmentAction!delAttachment.action',
				    	   data:{"attId":attId},
				    	   type:"post",
				    	   success:function(data){
				    		   $.messager.alert("提示","删除成功!");
				    		   $(obj).parent().remove();
				    	   },
				    	   error:function(){
				    		   $.messager.alert("提示","删除失败!");
				    	   }
				       });
			    }   
			});  
		}
		
		//上传附件方法
		function fileUploads(obj){
			var id = $(obj).parent().prev().children().attr("id");
			var listId = $(obj).parent().prev().prev().attr("id");
			var cDivClone = $("#"+id).children("div:first");
			var auditFormId = $("#"+id).parent().parent().attr("id");
			var auditId = $("#"+auditFormId+" input[name='auditId']").val();
			if(""==auditId){
				$.messager.alert("提示","请填先填写稽核信息!","info");
				return false;
			}
			var cDiv = $("#"+id).children();
			var att_types = ""; //附件类型
			var fileNames = ""; //附件名
			var fileIds = [];	//附件id
			for(var i = 0 ; i < cDiv.length; i++){
				var cDivId = cDiv[i].id;
				var att_type = $("#"+cDivId+" input:first").combobox("getValue");
				var fileName = $("#"+cDivId+" input[name='fileName']").val();
				var fileId = $("#"+cDivId+" input:last").attr("id");
				var fileValue = document.getElementById(fileId).value;
				if(""==att_type || ""==fileName || ""==fileValue){
					$.messager.alert("提示","请填写完整信息","info");
					return false;
				}
				att_types  += att_type + ",";
				fileNames += fileName + ",";
				fileIds[i] = fileId;
			}
			$.ajaxFileUpload({
				url:'attachment/attachmentAction!saveAttachment.action',
				data:{"fileName":fileNames,"attType":att_types,"loanOrderId" : $row.loanOrderId,"auditId":auditId},
				fileElementId:fileIds,
				secureuri:false,
				dataType:'text',
				async : false,
				success:function(data,status){
					loadAttachmentList(listId,auditId);
					$.messager.alert("提示","上传成功");
					$("#"+id).empty().append(cDivClone);
					$(cDivClone).children("a:first").click();
					$(cDivClone).remove();
				}
			});
		}
		
		//增加一条公司信息 
		function addACompany(obj) {
			j++;
			/* var pDiv = obj.parentElement.parentElement;
			var pDivId = pDiv.id.split("_")[0];
			$(pDiv.parentElement).append(
					"<div id='"+pDivId+"_"+j+"'>" + $(pDiv).html() + "</div>");
			var pFormId = obj.parentElement.id;
			$("#" + pDivId + "_" + j + " form").attr("id", pFormId + "_" + j);
			$("#" + pFormId+"_"+j+" input:first").val("");
			$("#" + pFormId+"_"+j+" div:eq(1)").empty().attr("id","companyInfoAttrList"+j); */
			var divId = "companyInfoModelDiv_"+j;
			var formId = "companyInfoModel_"+j;
			$("#companyInfoModel").clone().attr({"id":divId,"style":""}).appendTo("#comitIversTel_upload_father_idDiv");
			$("#"+divId).children("form").attr("id",formId);
			$("#"+formId+" a:eq(0)").attr({"class":"easyui-linkbutton","iconCls":"icon-add","plain":"true"});			
			$("#"+formId+" a:eq(1)").attr({"class":"easyui-linkbutton","iconCls":"icon-remove","plain":"true"});			
			$("#"+formId+" table input:eq(0)").attr({"class":"easyui-textbox easyui-validatebox","data-options":"required:true"});			
			$("#"+formId+" table input:eq(1)").attr({"class":"easyui-textbox easyui-validatebox","data-options":"required:true"});			
			$("#"+formId+" table textarea:eq(0)").attr("class","easyui-textbox");			
			$("#"+formId+" table textarea:eq(1)").attr("class","easyui-textbox");			
			$("#"+formId+" a:eq(2)").attr({"class":"easyui-linkbutton","iconCls":"icon-save"});
			$("#"+formId+" div:eq(1)").attr("id","companyInfoAttrList"+j);
			$("#"+formId+" div:eq(2)").attr("id","companyInfo_upload_add"+j);
			$("#"+formId+" div:eq(3)").attr("id","companyInfo_upload_father_idDiv"+j);
			$("#"+formId+" div:eq(4)").attr("id","companyInfo_upload_div"+j);
			$("#companyInfo_upload_div"+j+" input:eq(0)").attr("class","easyui-combobox");
			$("#companyInfo_upload_div"+j+" input:eq(2)").attr("class","easyui-textbox");
			$("#companyInfo_upload_div"+j+" a:eq(0)").attr({"class":"easyui-linkbutton","iconCls":"icon-add","plain":"true"});			
			$("#companyInfo_upload_div"+j+" a:eq(1)").attr({"class":"easyui-linkbutton","iconCls":"icon-remove","plain":"true"});			
			$("#"+formId+" a:last").attr("class","easyui-linkbutton");
			
			$.parser.parse('#'+formId); 
			
		    $("#companyInfo_upload_div"+j+" input:eq(0)").combobox({
				valueField : 'code',
				textField : 'text',
				data : attempData,
				editable:false ,
		    }); 
		}

		//删除一条公司信息
		function removeACompany(obj) {
			var pDiv = obj.parentElement.parentElement;
			var ppDiv = $(obj).parent().parent().parent();
			if (ppDiv.children().length == 1) {
				return false;
			}
			$(pDiv).remove();
		}
		
		//填写稽核信息记录
		function auditInfoRecord() {
			//根据订单ID 查询配偶 名称 电话 
			$.ajax({
					url : "contacts/contactsAction!findContactByOid.action",
					data : {
						loanOrderId : $row.loanOrderId
					},
					async: false,
					type : "post",
					success : function(data) {
						$("#spauseInfo-form input[name='name']").val(
								data.chName);
						$("#spauseInfo-form input[name='contactMethod']").val(
								data.tel);
					},
					error:function(){
						
					}
					
				});

			//根据订单ID查询公司信息
			$.ajax({
				url : "company/companyAction!findCompanyByLoanerId.action",
				data : {
					loanOrderId : $row.loanOrderId,
				},
				async: false,
				type : "post",
				success : function(data) {
					$("#contactWayInfoC-form input[name='contactMethod']").val(data[0].tele);
					if(data.length>0){
						$("#comitIversTel_upload__div").remove();
						$.each(data,function(i,item){
							addACompany();
						}); 
					}
					var comFormDivs = $("#comitIversTel_upload_father_idDiv").children();
					for(var i = 0 ; i < comFormDivs.length; i++){
						var formId = $("#"+comFormDivs[i].id+" form").attr("id");
						//alert(formId);
						//console.info(data[i]);
						$("#"+formId+" input[name='comId']").val(data[i].comId);
						$("#"+formId+" input[name='name']").val(data[i].name);
					}
				},
				error:function(){
					
				}
			});
			
			//查询稽核信息记录
			$.ajax({
				url : "auditInfoRecord/auditInfoRecordAction!findAIRByOid.action",
				data : {
					loanOrderId : $row.loanOrderId
				},
				type : "post",
				async: false,
				success : function(data) {
					var auditItem = "";
					var auditId = "";
					$.each(data,function(i,item){
						auditItem = item.auditItem;
						auditId = item.auditId;
						if("audit_applicant"==auditItem){
							$("#loanerInfo-form").form('load',item);
						}else if("audit_spouse"==auditItem){
							$("#spauseInfo-form").form('load',item);
						}else if("audit_home_phone"==auditItem){
							$("#contactWayInfoH-form").form('load',item);
						}else if("audit_workplace_phone"==auditItem){
							$("#contactWayInfoC-form").form('load',item);
						}else if("audit_cell_phone"==auditItem){
							$("#contactWayInfoZP-form").form('load',item);
						}else if("audit_fix_phone"==auditItem){
							$("#contactWayInfoZT-form").form('load',item);
						}else if("audit_upstream_partner"==auditItem){
							$("#companyAUP-form").form('load',item);
						}else if("audit_downstream_partner"==auditItem){
							$("#companyADP-form").form('load',item);
						}/* else if("audit_company"==auditItem){
							$("#companyInfo-form").form('load',item);
						} */
					});
				},
				error:function(){
					
				}
			});
			
			// 渲染用户的名称
			$("#loanerInfo-form input[name='name']").val($row.name);
			// 渲染手机号码
			$("#loanerInfo-form input[name='contactMethod']").val($row.mobileTel);
			//渲染联系方式信息 - 用户住宅电话
			$("#contactWayInfoH-form input[name='contactMethod']").val(
					$row.fixedTel);
			//渲染联系方式信息 - 用户单位电话 
			/* $("#contactWayInfo-form input[name='fixedTel']").val($row.fixedTel); */
			var attachmentTypeCombo = $(".attachmentTypeCombo");
			for(var k = 0 ; k < attachmentTypeCombo.length;k++){
				var aTCId = attachmentTypeCombo[k].id;
				$("#"+aTCId+" input:first").combobox({
					valueField: 'code',
					textField: 'text',
					data : attempData,
					editable:false ,
				});
			}
			var auditId;
			//获取客户附件
			auditId = $("#loanerInfo-form input:first").val();
			loadAttachmentList('customerAttList',auditId);
			
			//获取配偶附件
			auditId = $("#spauseInfo-form input:first").val();
			loadAttachmentList('spauseAttList',auditId);
			
			//获取家庭固话附件
			auditId = $("#contactWayInfoH-form input:first").val();
			loadAttachmentList('contactWayHAttrList',auditId);
			
			//获取单位固话附件
			auditId = $("#contactWayInfoC-form input:first").val();
			loadAttachmentList('contactWayCAttrList',auditId);
			
			//获取征信手机附件
			auditId = $("#contactWayInfoZP-form input:first").val();
			loadAttachmentList('contactWayZPAttrList',auditId);
			
			//获取征信电话附件
			auditId = $("#contactWayInfoZT-form input:first").val();
			loadAttachmentList('contactWayZTAttrList',auditId);

			//获取联系人信息附件
			/* auditId = $("#contacts-form-model input:first").val();
			loadAttachmentList('contacts',auditId); */

			//获取公司信息附件
			auditId = $("#companyInfo-form input:first").val();
			loadAttachmentList('companyInfoAttrList',auditId); 
			
			//获取上游合作单位附件
			auditId = $("#companyAUP-form input:first").val();
			loadAttachmentList('companyAUPAttrList',auditId);
			
			//获取下游合作单位附件
			auditId = $("#companyADP-form input:first").val();
			loadAttachmentList('companyADPAttrList',auditId); 
			
			$("#auditInfoRecordDialog").dialog("open");
		}
		
		//表单禁用
		function disableForm(id){
			var  myArrays=$("#"+id+" input[class^='easyui-']");
		    for(var i = 0;i<myArrays.length;i++){
		       if($(myArrays[i]).hasClass("easyui-aa")){
		     	   $(myArrays[i]).combobox("disable");
		       }else if($(myArrays[i]).hasClass("easyui-combotree")){
		   		   $(myArrays[i]).combotree("disable");
		   	   }else if($(myArrays[i]).hasClass("easyui-combogrid")){
		   		   $(myArrays[i]).combogrid("disable");
		   	   }else if($(myArrays[i]).hasClass("easyui-combobox")){
		   		   $(myArrays[i]).combobox("disable");
		   	   }else if ($(myArrays[i]).hasClass("easyui-textbox")){
		  		   $(myArrays[i]).attr("disabled",true);
		  	   }else{
		  	   }
		     }
		}
		
