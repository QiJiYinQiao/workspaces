	// 提交表单信息
	function  submitTask(result) {
		var data = {
			"comment" : $("#comment").val(),
			"result" :result,
			"loanOrderId" : $row.loanOrderId,
		}
		$.ajax({
			type : "POST",
			url : "loanOrder/loanOrderAction!submitTask.action",
			data : data,
			success : function(msg) {
				parent.$.modalDialog.openner.datagrid('reload');
				parent.$.modalDialog.handler.dialog('close');
			}
		});
	}
		
	function saveAuditInfo(obj) {
		var id = obj.parentElement.parentElement.id;
		$('#' + id).form(
				'submit',
				{
					url : "auditInfoRecord/auditInfoRecordAction!saveAuditInfoRecord.action",
					onSubmit: function(param){
						var isValid = $(this).form('validate');
						if (isValid){
							param.loanOrderId = $row.loanOrderId;
						}
						return isValid;	//  返回false将停止form提交 
					},
					success : function(data) {
						data = $.parseJSON(data);
						$("#" + id + " input:first").val(data.data.auditId); 
						$.messager.alert(data.title,data.message,"info");
					}
				});

	}
	
	function airdClose(){
		$("#auditInfoRecordDialog").dialog('refresh');
	}
	
	$("#auditTabs").tabs({
		onSelect:function(title,index){
			alert(index);
		},
	})
	
</script>
