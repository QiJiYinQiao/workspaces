										return result;
									}
								}
							} 
							] ],
					toolbar : '#tb'
				});
		
		
			//贷款类型
		   $("#loanType").combobox({
				url:"common/commonAction!findTextArr.action?codeMyid=loan_type",
				valueField: 'code',
				textField: 'text',
				editable:false,
		   });
		
