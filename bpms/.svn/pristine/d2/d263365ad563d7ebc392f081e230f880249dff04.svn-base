	//查询
	function queryLoanOrder(){
		$grid.datagrid("load",{
				"name":$("#loanerName").val(),
				"mobileTel":$("#mobileTel").val(),
				"idNo":$("#idNo").val(),
				"loanType":$("#loanType").combobox("getValue")
			});
	}
	
	//重置查询条件
	function resetQueryCondition(){
		$("#queryLoanOrderForm").form("clear");
		$grid.datagrid("load",{});
	}
	
