package com.bpms.mydao.loan;

import java.util.List;
import java.util.Map;

/**
 * 工作单位地址的查询
 * 
 * @author liuhh
 *
 */
public interface CompanyMapper {

	/**
	 * 根据查询获取指定的工作单位的信息
	 * 
	 * @param param
	 *            查询参数
	 * @return 获取指定工作单位的列表
	 */
	List<Map<String, Object>> findCompanyList(Map<String, Object> param);

	/**
	 * 根据查询获取指定单位的个数的信息
	 * 
	 * @param param
	 *            查询参数
	 * @return 获取指定单位的列表个数
	 */
	long findCompanyListCount(Map<String, Object> param);
}
